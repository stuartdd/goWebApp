{
    "port": 8083,
    "ServerName": "TestServer",
    "ReloadConfigSeconds": 3600,
    "filterFiles": [
        "Json",
        "Data",
        "jpeg",
        "log"
    ],
    "UserPropertiesFile": "",
    "ContentTypeCharset": "utf-8",
    "FaviconIcoPath": "favicon1.ico",
    "LogData": {
        "FileNameMask": "goWebServer-%y-%m-%d-%H-%M.log",
        "Path": "logs",
        "MonitorSeconds": 30,
        "ShowPathInStatus": 2,
        "LogLevel": "quiet"
    },
    "Exec": {
        "lr1": {
            "Cmd": [
                "longRunTest1.sh"
            ],
            "Description": "Test long running process",
            "StartLTSFile": "longRunTest1.txt",
            "Detached": true,
            "CanStop": true
        },
        "ls": {
            "cmd": [
                "ls",
                "-lta"
            ],
            "logDir": "logs",
            "logOutFile": "stdOutLs.txt",
            "logErrFile": "stdErrLs.txt"
        },
        "free": {
            "StdOutType": "json",
            "Cmd": [
                "free.sh"
            ]
        },
        "c2": {
            "cmd": [
                "cmd2"
            ],
            "dir": "",
            "logDir": "logs",
            "logOutFile": "stdOutC2.txt",
            "logErrFile": "stdErrC2.txt"
        },
        "cat": {
            "cmd": [
                "cat",
                "fileThatDoesNotExist"
            ],
            "dir": "",
            "nzCodeReturns": 403,
            "logDir": "logs",
            "logOutFile": "stdOutCat.txt",
            "logErrFile": "stdErrCat.txt"
        }
    },
    "Users": {
        "bob": {
            "name": "Bob",
            "home": "bob",
            "Locations": {
                "home": "",
                "usr": "b-testfolder",
                "pics": "b-pics"
            }
        },
        "stuart": {
            "name": "Stuart",
            "home": "stuart",
            "Locations": {
                "home": "",
                "logs": "logs",
                "usr": "s-testfolder",
                "pics": "s-pics",
                "testtree": "",
                "picsMissing": "missingfolder",
                "picsPlus": "s-pics/s-testfolder"
            }
        }
    },
    "ServerDataRoot": "%{WebServerUserData}",
    "StaticData": {
        "Path": "../testdata/static",
        "HomePage": "tgo.html"
    },
    "TemplateStaticFiles": {
        "Files": [
            "tgo.html",
            "tgoimage.html",
            "tgo.css"
        ],
        "DataFile": "configDataPI.json"
    },
    "ExecPath": "../testdata/exec",
    "Env": {
        "TestTemplate": "This is a test"
    }
}